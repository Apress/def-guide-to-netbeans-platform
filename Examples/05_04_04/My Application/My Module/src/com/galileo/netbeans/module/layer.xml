<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Edit">
            <file name="com-galileo-netbeans-module-MyAction1.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.galileo.netbeans.module.Bundle"/>
                <attr name="delegate" newvalue="com.galileo.netbeans.module.MyAction1"/>
                <attr name="displayName" bundlevalue="com.galileo.netbeans.module.Bundle#CTL_MyAction1"/>
                <attr name="instanceCreate" methodvalue="org.openide.awt.Actions.alwaysEnabled"/>
                <attr name="noIconInMenu" stringvalue="false"/>
            </file>
            <file name="com-galileo-netbeans-module-MyAction2.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.galileo.netbeans.module.Bundle"/>
                <attr name="delegate" newvalue="com.galileo.netbeans.module.MyAction2"/>
                <attr name="displayName" bundlevalue="com.galileo.netbeans.module.Bundle#CTL_MyAction2"/>
                <attr name="instanceCreate" methodvalue="org.openide.awt.Actions.alwaysEnabled"/>
                <attr name="noIconInMenu" stringvalue="false"/>
            </file>
        </folder>
        <folder name="Window">
            <file name="com-galileo-netbeans-module-MyAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.galileo.netbeans.module.Bundle"/>
                <attr name="component" methodvalue="com.galileo.netbeans.module.MyTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="com.galileo.netbeans.module.Bundle#CTL_MyAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="MyAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/com-galileo-netbeans-module-MyAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="MyTopComponent.settings" url="MyTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="MyTopComponent.wstcref" url="MyTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
   <folder name="ContextActions">
      <folder name="MyTC">
         <file name="MyAction1.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/com-galileo-netbeans-module-MyAction1.instance"/>
            <attr name="position" intvalue="20"/>
         </file>
         <file name="MyAction2.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/com-galileo-netbeans-module-MyAction2.instance"/>
